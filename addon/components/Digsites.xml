					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$parent" x="12" y="-20"/>
				</Anchors>
				<Scripts>
					<OnLoad>
						self.bg = self:CreateTexture(nil, "BACKGROUND");
						self.bg:SetAllPoints(true);
